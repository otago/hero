{
    "name": "otago/hero",
    "description": "A skeleton for SilverStripe modules.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "gorriecoe/silverstripe-linkfield": "^1.0",
        "gorriecoe/silverstripe-link": "^1.4",
        "symbiote/silverstripe-gridfieldextensions": "^3.5",
        "otago/tepukenga-header": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeModule\\Skeleton\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
